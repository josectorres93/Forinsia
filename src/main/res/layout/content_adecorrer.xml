<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="app.insia.forinsiaapp.ADecorrerActivity"
    tools:showIn="@layout/activity_adecorrer">

    <TextView
        android:id="@+id/n1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="102dp"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/d1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/n1"
        android:layout_centerHorizontal="true"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/l1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/d1"
        android:layout_marginEnd="30dp"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/n2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/n1"
        android:layout_below="@+id/n1"
        android:layout_marginTop="61dp"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/d2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/d1"
        android:layout_alignTop="@+id/n2"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/l2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/l1"
        android:layout_alignTop="@+id/d2"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/n3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/n2"
        android:layout_below="@+id/n2"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/d3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/d2"
        android:layout_alignTop="@+id/n3"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/d3"
        android:layout_alignStart="@+id/l2"
        android:text="TextView"
        android:visibility="invisible" />
</RelativeLayout>
